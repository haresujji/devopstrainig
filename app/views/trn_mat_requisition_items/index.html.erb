<p id="notice"><%= notice %></p>

<h1>Trn Mat Requisition Items</h1>

<table>
  <thead>
    <tr>
      <th>Req no</th>
      <th>Mat code</th>
      <th>Mat desc</th>
      <th>Mat uom</th>
      <th>Mat qty</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @trn_mat_requisition_items.each do |trn_mat_requisition_item| %>
      <tr>
        <td><%= trn_mat_requisition_item.req_no %></td>
        <td><%= trn_mat_requisition_item.mat_code %></td>
        <td><%= trn_mat_requisition_item.mat_desc %></td>
        <td><%= trn_mat_requisition_item.mat_uom %></td>
        <td><%= trn_mat_requisition_item.mat_qty %></td>
        <td><%= link_to 'Show', trn_mat_requisition_item %></td>
        <td><%= link_to 'Edit', edit_trn_mat_requisition_item_path(trn_mat_requisition_item) %></td>
        <td><%= link_to 'Destroy', trn_mat_requisition_item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Trn Mat Requisition Item', new_trn_mat_requisition_item_path %>
